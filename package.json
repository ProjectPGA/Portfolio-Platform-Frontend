    "core-js": "^3.8.3",
    "register-service-worker": "^1.7.2",
    "style-resources-loader": "^1.4.1",
    "tslint-config-prettier": "^1.18.0",
    "vee-validate": "^3.4.5",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vue-i18n": "^8.22.4",
    "vue-mq": "^1.0.1",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2",
